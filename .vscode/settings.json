{
    "files.exclude": {
        "**/*.css": { "when": "$(basename).less" },
        "**/*.css.map": { "when": "$(basename).less" },
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true
    }
}